# Google Maps API Key
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Configuration - Update with your Railway backend URL
# Replace 'your-railway-backend-url' with your actual Railway deployment URL
REACT_APP_API_URL=https://your-railway-backend-url.railway.app
REACT_APP_SOCKET_URL=wss://your-railway-backend-url.railway.app

# Alternative: Use existing Render backend (currently configured)
# REACT_APP_API_URL=https://food-zone-backend-l00k.onrender.com
# REACT_APP_SOCKET_URL=wss://food-zone-backend-l00k.onrender.com

# Environment
NODE_ENV=production
